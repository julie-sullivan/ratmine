<?xml version="1.0"?>
<userprofile>
  <items/>
  <bags/>
  <queries>
  </queries>
<template-queries>
<template longDescription="Find an entry in the database using any identifier or synonym." name="A_IdentifierSynonym_Object" comment="This is the template that the quick search runs.  If the name is changed it also has to be changed for the quick search configuration - Rachel. Removed tags so no longer displayed - just used for quick search now.  Rachel." title="Identifier or synonym --&gt; Database object."><query longDescription="Find an entry in the database using any identifier or synonym." view="Synonym.subject.primaryIdentifier Synonym.value Synonym.subject.organism.name" name="A_IdentifierSynonym_Object" model="genomic" sortOrder="Synonym.subject.primaryIdentifier asc"><constraint value="timeout" path="Synonym.value" identifier="Synonym.value.1" editable="true" switchable="locked" op="=" code="A"/></query></template>
<template longDescription="Search for genes in a specified organism that is associated with a particular GO annotation." name="GO_name_Gene" comment="" title="GO term name/id --&gt; Genes in one specific organism."><query longDescription="Search for genes in a specified organism that is associated with a particular GO annotation." name="GO_name_Gene" model="genomic" constraintLogic="A and B" view="Gene.primaryIdentifier Gene.symbol Gene.goAnnotation.ontologyTerm.identifier Gene.goAnnotation.ontologyTerm.name" sortOrder="Gene.symbol asc"><pathDescription description="GO annotation" pathString="Gene.goAnnotation"/><constraint value="Plasmodium falciparum 3D7" path="Gene.organism.name" description="Search for genes in:" editable="true" switchable="locked" code="B" op="="/><constraint value="*DNA binding*" path="Gene.goAnnotation.ontologyTerm.name" description="GO Term" editable="true" switchable="locked" code="A" op="="/></query></template>
<template longDescription="Find the GO identifier for a specific term." name="GOname_GOidentifier" comment="" title="GO term name --&gt; GO term identifier."><query longDescription="Find the GO identifier for a specific term." name="GOname_GOidentifier" model="genomic" view="GOTerm.name GOTerm.identifier GOTerm.description" sortOrder="GOTerm.name asc"><constraint value="*blastoderm*" path="GOTerm.name" editable="true" switchable="locked" code="A" op="="/></query></template>
<template longDescription="Give the GO term name for a particular GO term identifier. (Data Source: GO)" name="GOtermidentifier_name" comment="" title="GO term identifier --&gt; GO term name."><query longDescription="Give the GO term name for a particular GO term identifier. (Data Source: GO)" name="GOtermidentifier_name" model="genomic" view="GOTerm.identifier GOTerm.name GOTerm.description" sortOrder="GOTerm.identifier asc"><constraint value="GO:0007350" path="GOTerm.identifier" editable="true" switchable="locked" code="A" op="="/></query></template>
<template name="Gene_GO" title="Gene --&gt; GO terms." longDescription="Search for GO annotations for a particular gene." comment="06.06.07 updated to work from gene class - Philip">
  <query name="Gene_GO" model="genomic" view="Gene.primaryIdentifier Gene.secondaryIdentifier Gene.symbol Gene.goAnnotation.goEvidenceCodes.code Gene.goAnnotation.qualifier Gene.goAnnotation:ontologyTerm.identifier Gene.goAnnotation:ontologyTerm.name Gene.goAnnotation:ontologyTerm.namespace" sortOrder="Gene.primaryIdentifier asc">
    <pathDescription pathString="Gene.goAnnotation" description="GO annotation">
    </pathDescription>
    <pathDescription pathString="Gene.goAnnotation.goEvidenceCodes" description="GO annotation &gt; evidence">
    </pathDescription>
    <node path="Gene" type="Gene">
      <constraint op="LOOKUP" value="CG11348" description="Show GO annotations for gene:" identifier="Gene.class" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
  </query>
</template>

<template longDescription="Show all identifiers for a gene, given an identifier." name="Gene_Identifiers" comment="07:02:07:Updated to work from gene identifier - Rachel." title="Gene --&gt; All identifiers."><query longDescription="Show all identifiers for a gene, given an identifier." name="Gene_Identifiers" model="genomic" view="Gene.secondaryIdentifier Gene.primaryIdentifier Gene.symbol Gene.name" sortOrder="Gene.secondaryIdentifier asc"><constraint value="PFL2660w" path="Gene.secondaryIdentifier" description="Gene.secondaryIdentifier" identifier="Gene.secondaryIdentifier" editable="true" switchable="locked" code="A" op="="/></query></template>
<template longDescription="Show the proteins for a particular gene." name="Gene_Protein" comment="06.06.07 updated to work from gene class - Philip" title="Gene --&gt; Proteins."><query longDescription="Show the proteins for a particular gene." name="Gene_Protein" model="genomic" view="Gene.primaryIdentifier Gene.symbol Gene.proteins.primaryIdentifier Gene.proteins.primaryAccession" sortOrder="Gene.primaryIdentifier asc"><pathDescription description="Protein" pathString="Gene.proteins"/><constraint value="PFL2660w" path="Gene" editable="true" switchable="locked" code="A" op="LOOKUP"/></query></template>
<template longDescription="" name="Gene_To_Protein_Type_Converter" comment="" title="Gene to protein type converter"><query name="Gene_To_Protein_Type_Converter" model="genomic" view="Gene.id Gene.proteins.id" sortOrder="Gene.id asc"><constraint value="0" path="Gene.id" description="Gene.id" identifier="Gene.id" editable="true" switchable="locked" code="A" op="="/></query></template>
<template name="Gene_allGOTerms" title="Gene --&gt; GO terms and parents of those terms." longDescription="Given a gene show the associated GO terms, and the all parents to the top of the directed graph." comment="">
  <query name="Gene_allGOTerms" model="genomic" view="Gene.secondaryIdentifier Gene.symbol Gene.goAnnotation.ontologyTerm.relations.childTerm.identifier Gene.goAnnotation.ontologyTerm.relations.childTerm.name Gene.goAnnotation.ontologyTerm.relations.relationship Gene.goAnnotation.ontologyTerm.relations.parentTerm.identifier Gene.goAnnotation.ontologyTerm.relations.parentTerm.name Gene.goAnnotation.goEvidenceCodes.code" sortOrder="Gene.secondaryIdentifier asc">
    <pathDescription pathString="Gene.goAnnotation" description="GO annotation">
    </pathDescription>
    <pathDescription pathString="Gene.goAnnotation.goEvidenceCodes" description="GO annotation &gt; evidence">
    </pathDescription>
    <node path="Gene" type="Gene">
      <constraint op="LOOKUP" value="PFL2660w" description="Show GO annotations for gene:" identifier="Gene.class" editable="true" code="A" extraValue="">
      </constraint>
    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes" type="GOEvidenceCode">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm" type="OntologyTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.relations" type="OntologyRelation">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.relations.childTerm" type="OntologyTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.relations.parentTerm" type="OntologyTerm">
    </node>
  </query>
</template>

<template longDescription="Show if the following gene has the following GO term." name="Gene_particularGoannotation" comment="" title="Gene + GO term name --&gt; Gene with the GO term name."><query longDescription="Show if the following gene has the following GO term." name="Gene_particularGoannotation" model="genomic" constraintLogic="A and B" view="Gene.secondaryIdentifier Gene.symbol Gene.goAnnotation.ontologyTerm.identifier Gene.goAnnotation.ontologyTerm.name" sortOrder="Gene.secondaryIdentifier asc"><join style="OUTER" path="Gene.goAnnotation"/><join style="OUTER" path="Gene.goAnnotation"/><join style="OUTER" path="Gene.goAnnotation"/><join style="OUTER" path="Gene.goAnnotation"/><join style="OUTER" path="Gene.goAnnotation"/><constraint value="CG9015" path="Gene" description="For the gene(s):" editable="true" switchable="locked" code="A" op="LOOKUP"/><constraint value="*transcription factor*" path="Gene.goAnnotation.ontologyTerm.name" description="GO Term" editable="true" switchable="locked" code="B" op="="/></query></template>
<template name="Organism_GoTerm_Gene" title="Organism + GO term [with particular evidence code] --&gt; Genes." longDescription="Show genes from a particular organism annotated with a particular GO term with a particular evidence code." comment="">
  <query name="Organism_GoTerm_Gene" model="genomic" view="Gene.secondaryIdentifier Gene.symbol Gene.goAnnotation.ontologyTerm.identifier Gene.goAnnotation.ontologyTerm.name Gene.goAnnotation.goEvidenceCodes.code" sortOrder="Gene.secondaryIdentifier asc" constraintLogic="A and B and C">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.organism" type="Organism">
    </node>
    <node path="Gene.organism.name" type="String">
      <constraint op="=" value="Plasmodium falciparum 3D7" description="Show genes from organism:" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm" type="OntologyTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.name" type="String">
      <constraint op="=" value="protein binding" description="GO Term" identifier="" editable="true" code="B" extraValue="">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes" type="GOEvidenceCode">
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes.code" type="String">
      <constraint op="=" value="IPI" description="GO Evidence Code" identifier="" editable="true" code="C" extraValue="">
      </constraint>
    </node>
  </query>
</template>

<template name="Organism_GoTerm_GeneWithannotation" title="Organism + GO term [with particular evidence code] --&gt; Genes + 'with' annotation." longDescription="Show genes from a particular organism annotated with a particular GO term and GO evidence code which also have a 'with' annotation (The 'with' field holds an additional identifier for annotations using certain evidence codes)" comment="">
  <query name="Organism_GoTerm_GeneWithannotation" model="genomic" view="Gene.secondaryIdentifier Gene.symbol Gene.goAnnotation.ontologyTerm.identifier Gene.goAnnotation.ontologyTerm.name Gene.goAnnotation.goEvidenceCodes.code Gene.goAnnotation.withText Gene.goAnnotation:with.primaryIdentifier" sortOrder="Gene.secondaryIdentifier asc" constraintLogic="A and B and C">
    <node path="Gene" type="Gene">
    </node>
    <node path="Gene.organism" type="Organism">
    </node>
    <node path="Gene.organism.name" type="String">
      <constraint op="=" value="Plasmodium falciparum 3D7" description="Show genes and 'with' annotations from organism:" identifier="" editable="true" code="A">
      </constraint>
    </node>
    <node path="Gene.goAnnotation" type="GOAnnotation">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm" type="OntologyTerm">
    </node>
    <node path="Gene.goAnnotation.ontologyTerm.name" type="String">
      <constraint op="=" value="protein binding" description="GO Term" identifier="" editable="true" code="B" extraValue="">
      </constraint>
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes" type="GOEvidenceCode">
    </node>
    <node path="Gene.goAnnotation.goEvidenceCodes.code" type="String">
      <constraint op="=" value="IPI" description="GOEvidenceCode code" identifier="" editable="true" code="C" extraValue="">
      </constraint>
    </node>
  </query>
</template>

<template longDescription="Show all the proteins from a particular organism." name="Organism_Protein" comment="" title="Organism --&gt; All proteins."><query longDescription="Show all the proteins from a particular organism." name="Organism_Protein" model="genomic" view="Protein.primaryIdentifier Protein.primaryAccession" sortOrder="Protein.primaryIdentifier asc"><constraint value="Plasmodium falciparum 3D7" path="Protein.organism.name" description="Show all the proteins from the organism:" identifier="Organism.name" editable="true" switchable="locked" code="A" op="="/></query></template>
<template longDescription="Show all GO terms that have been associated with the gene that codes for the following protein." name="Protein_GOterms" comment="06.06.07 updated to work from protein class - Philip" title="Protein --&gt; GO terms."><query longDescription="Show all GO terms that have been associated with the gene that codes for the following protein." name="Protein_GOterms" model="genomic" view="Protein.primaryAccession Protein.genes.secondaryIdentifier Protein.genes.symbol Protein.genes.goAnnotation.ontologyTerm.namespace Protein.genes.goAnnotation.ontologyTerm.identifier Protein.genes.goAnnotation.ontologyTerm.name" sortOrder="Protein.primaryAccession asc"><pathDescription description="Gene" pathString="Protein.genes"/><join style="OUTER" path="Protein.genes.goAnnotation.ontologyTerm"/><join style="OUTER" path="Protein.genes.goAnnotation.ontologyTerm"/><join style="OUTER" path="Protein.genes.goAnnotation.ontologyTerm"/><constraint value="B3FEM5" path="Protein" identifier="Protein.class" editable="true" switchable="locked" code="A" op="LOOKUP"/></query></template>
<template longDescription="Show the GO annotations for a protein or proteins from a specifc GO ontology (cellular_component, molecular_function, biological_process)." name="Protein_GOtermsSpecificOntology" comment="" title="Protein --&gt; GO terms from a specific GO ontology."><query longDescription="Show the GO annotations for a protein or proteins from a specifc GO ontology (cellular_component, molecular_function, biological_process)." name="Protein_GOtermsSpecificOntology" model="genomic" constraintLogic="A and B" view="Protein.primaryAccession Protein.primaryIdentifier Protein.genes.primaryIdentifier Protein.genes.secondaryIdentifier Protein.genes.symbol Protein.genes.goAnnotation.ontologyTerm.namespace Protein.genes.goAnnotation.ontologyTerm.identifier Protein.genes.goAnnotation.ontologyTerm.name" sortOrder="Protein.primaryAccession asc"><pathDescription description="Gene" pathString="Protein.genes"/><constraint extraValue="D. melanogaster" value="B3FEM5" path="Protein" description="Show GO annotaion for the following protein(s):" editable="true" switchable="locked" code="A" op="LOOKUP"/><constraint value="cellular_component" path="Protein.genes.goAnnotation.ontologyTerm.namespace" description="from the ontology:" editable="true" switchable="locked" code="B" op="="/></query></template>
<template longDescription="" name="Protein_To_Gene_Type_Converter" comment="" title="Protein to gene type converter"><query name="Protein_To_Gene_Type_Converter" model="genomic" view="Protein.id Protein.genes.id" sortOrder="Protein.id asc"><constraint value="0" path="Protein.id" description="Protein.id" identifier="Protein.id" editable="true" switchable="locked" code="A" op="="/></query></template>
</template-queries>
